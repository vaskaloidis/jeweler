<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Language</th>
      <th>Phase total</th>
      <th>Phase current</th>
      <th>Description</th>
      <th>Github url</th>
      <th>Github secondary url</th>
      <th>Readme file</th>
      <th>Readme remote</th>
      <th>Stage website url</th>
      <th>Demo url</th>
      <th>Prod url</th>
      <th>Complete</th>
      <th>Stage travis api url</th>
      <th>Stage travis api token</th>
      <th>Prod travis api token</th>
      <th>Prod travis api url</th>
      <th>Coveralls api url</th>
      <th>Customers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.language %></td>
        <td><%= project.phase_total %></td>
        <td><%= project.phase_current %></td>
        <td><%= project.description %></td>
        <td><%= project.github_url %></td>
        <td><%= project.github_secondary_url %></td>
        <td><%= project.readme_file %></td>
        <td><%= project.readme_remote %></td>
        <td><%= project.stage_website_url %></td>
        <td><%= project.demo_url %></td>
        <td><%= project.prod_url %></td>
        <td><%= project.complete %></td>
        <td><%= project.stage_travis_api_url %></td>
        <td><%= project.stage_travis_api_token %></td>
        <td><%= project.prod_travis_api_token %></td>
        <td><%= project.prod_travis_api_url %></td>
        <td><%= project.coveralls_api_url %></td>
        <td><%= project.customers %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
