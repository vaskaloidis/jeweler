<%= row do %>
  <%= column3 do %>
    <div class="xe-widget xe-counter" data-count=".num" data-from="0" data-to="<%= project.users.count %>" data-suffix=" users" data-duration="2">
      <div class="xe-icon">
        <i class="linecons-user"></i>
      </div>
      <div class="xe-label">
        <strong class="num">99.9%</strong>
        <span>Server uptime</span>
      </div>
    </div>
  <% end %>
  <%= column3 do %>
    <div class="xe-widget xe-counter xe-counter-blue" data-count=".num" data-from="0" data-to="<%= project.users.count %>" data-suffix=" users" data-duration="2">
      <div class="xe-icon">
        <i class="linecons-user"></i>
      </div>
      <div class="xe-label">
        <strong class="num">99.9%</strong>
        <span>Server uptime</span>
      </div>
    </div>
  <% end %>
  <%= column3 do %>
