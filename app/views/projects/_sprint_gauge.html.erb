<%
progress = (@project.sprint_current / @project.sprint_total) * 100
%>
<div class="panel-body">
  <h4>Development Cycle</h4>
  <div class="row">
    <div class="col-md-12">
      <div class="progress progress-striped">
        <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="<%= 50 %>" aria-valuemin="0" aria-valuemax="50" style="width: 100%">
          <span class="sr-only">40% Complete (success)</span>
        </div>
      </div>
    </div>
  </div>
</div>