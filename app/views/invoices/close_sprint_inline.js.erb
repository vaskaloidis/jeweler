<% if @sprint.valid? %>
<%= render 'invoices/re-render_invoice_panel', invoice: @sprint %>
<%= render 'notes/re-render_timeline', notes: @sprint.project.sprint_notes, project: @sprint.project %>
  toastr.success('Sprint <%= @sprint.sprint.to_s %> Closed');
<% else %>
  toastr.error('Sprint <%= @sprint.sprint.to_s %> Could Not Be Closed');
<% end %>