$("#tasks_<%= @sprint.id.to_s %>_hidden_row").hide('slide')
<%= render 'sprints/re-render_sprint_panel', sprint: @sprint %>

<% if @errors.empty? %>
  toastr.success('Task Created Successfully');
<% else %>
  <% @errors.each do |error| %>
    toastr.error('Error creating Task: <%=  error %>');
  <% end %>
<% end %>