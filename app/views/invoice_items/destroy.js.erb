<%= render 'invoices/re-render_invoice_panel', invoice: @invoice_item.invoice %>

<% if @error_msgs.empty? %>
  toastr.success('Task Deleted');
  $('#task_row_<%= @invoice_item.id.to_s %>').hide('slide');
<% else %>
  <% @error_msgs.each do |error| %>
    toastr.error('<%= error %>');
  <% end %>
<% end %>